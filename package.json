{
  "name": "email-anayzer9",
  "version": "1.0.0",
  "description": "Email Anayzer 9",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "pe.gujc.email-anayzer9",
    "productName": "Email Anayzer 9",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    }
  },
  "dependencies": {
    "imap": "^0.8.19",
    "mailparser": "^3.5.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^26.0.12"
  }
}
